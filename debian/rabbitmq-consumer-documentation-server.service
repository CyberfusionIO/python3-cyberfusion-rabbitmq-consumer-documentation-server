[Unit]
Description=RabbitMQ Consumer Documentation Server
After=network.target

[Service]
Environment=HOST='::'
Environment=PORT=9012
ExecStart=/usr/bin/rabbitmq-consumer-documentation-server --host $HOST --port $PORT
Restart=on-failure
RestartSec=120
PrivateTmp=true

[Install]
WantedBy=multi-user.target
